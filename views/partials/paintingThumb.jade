a(href="/painting/#{painting._id}")
  div(class="archiveItem grid")
    - var count = 0;
    - for (var row = 0; row < x; row++)
      div(class="row")
        - for (var col = 0; col < y; col++)
          div(class="archiveCell" id="archive_#{row}#{col}" style={background: "#{painting.painting[count]}"})
          - count++;
